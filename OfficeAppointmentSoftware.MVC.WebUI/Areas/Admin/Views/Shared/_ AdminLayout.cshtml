<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="Mosaddek">
    <meta name="keyword" content="FlatLab, Dashboard, Bootstrap, Admin, Template, Theme, Responsive, Fluid, Retina">
    <link rel="shortcut icon" href="~/Areas/Admin/AdminContent/assets/img/font-awesome/favicon.png">
    <link href="~/Areas/Admin/AdminContent/assets/css/bootstrap.min.css" rel="stylesheet">

    <link href="~/Areas/Admin/AdminContent/assets/css/bootstrap-reset.css" rel="stylesheet">
    <!--external css-->
    <link href="~/Areas/Admin/AdminContent/assets/font-awesome/css/font-awesome.css" rel="stylesheet" />
    <link href="~/Areas/Admin/AdminContent/assets/jquery-easy-pie-chart/jquery.easy-pie-chart.css" rel="stylesheet" type="text/css" media="screen" />
    <link rel="stylesheet" href="~/Areas/Admin/AdminContent/assets/css/owl.carousel.css" type="text/css">
    <!-- Custom styles for this template -->
    <link href="~/Areas/Admin/AdminContent/assets/css/style.css" rel="stylesheet">
    <link href="~/Areas/Admin/AdminContent/assets/css/style-responsive.css" rel="stylesheet" />
    <link href="~/Areas/Admin/AdminContent/assets/css/WorksList.css" rel="stylesheet" />
    <link href="~/Areas/Admin/AdminContent/assets/css/WorksList.css" rel="stylesheet" />
    <!-- HTML5 shim and Respond.js IE8 support of HTML5 tooltipss and media queries -->
    <!--[if lt IE 9]>
      <script src="js/html5shiv.js"></script>
      <script src="js/respond.min.js"></script>
    <![endif]-->
</head>

<body>

    <section id="container">
        <!--header start-->
        <header class="header white-bg">
            <div class="sidebar-toggle-box">
                <div data-original-title="Toggle Navigation" data-placement="right" class="icon-reorder tooltips"></div>
            </div>
            <!--logo start-->
            <a href="index.html" class="logo">Cet<span>ris</span></a>
            <!--logo end-->
            <div class="nav notify-row" id="top_menu">

            </div>
            <div class="top-nav ">
                <!--search & user info start-->
                <ul class="nav pull-right top-menu">
                    @*<li>
                        <input type="text" class="form-control search" placeholder="Search">
                    </li>*@
                    <!-- user login dropdown start-->
                    <li class="dropdown">
                        <a data-toggle="dropdown" class="dropdown-toggle" href="#">
                            <img alt="" src="img/avatar1_small.jpg">
                            <span class="username">İşlemler</span>
                            <b class="caret"></b>
                        </a>
                        <ul class="dropdown-menu extended logout">
                            <div class="log-arrow-up"></div>
                            @*<li><a href="#"><i class=" icon-suitcase"></i>Profile</a></li>
                            <li><a href="#"><i class="icon-cog"></i> Settings</a></li>
                            <li><a href="#"><i class="icon-bell-alt"></i> Notification</a></li>*@
                            <li><a href="/Account/Login"><i class="icon-key"></i> Çıkış</a></li>
                        </ul>
                    </li>
                    <!-- user login dropdown end -->
                </ul>
                <!--search & user info end-->
            </div>
        </header>
        <!--header end-->
        <!--sidebar start-->
        <aside>
            <div id="sidebar" class="nav-collapse ">
                <!-- sidebar menu start-->
                <ul class="sidebar-menu" id="nav-accordion">


                    <li class="sub-menu">
                        <a class="active" href="@Url.Action("SignUp","Admin")">
                            <i class="icon-user"></i>
                            <span>Üyelik Ekleme</span>
                        </a>
                    </li>

                    <li class="sub-menu">
                        <a class="active" href="@Url.Action("Banner","Admin")">
                            <i class="icon-book"></i>
                            <span>Banner Güncelleme</span>
                        </a>
                    </li>

                    <li class="sub-menu">
                        <a class="active" href="@Url.Action("FeaturesAdd","Admin")">
                            <i class="icon-cogs"></i>
                            <span>Özellik Ekleme</span>
                        </a>
                    </li>
                    <li class="sub-menu">
                        <a class="active" href="@Url.Action("FeaturesList","Admin")">
                            <i class="icon-cogs"></i>
                            <span>Özellik Listeleme</span>
                        </a>
                    </li>
                    <li class="sub-menu">
                        <a class="active" href="@Url.Action("WorksAdd","Admin")">
                            <i class="icon-laptop"></i>
                            <span>Şirket Ekleme</span>
                        </a>
                    </li>
                    <li class="sub-menu">
                        <a class="active" href="@Url.Action("WorksList","Admin")">
                            <i class="icon-laptop"></i>
                            <span>Şirket Listeleme</span>
                        </a>
                    </li>
                </ul>
                <!-- sidebar menu end-->
            </div>
        </aside>
        <!--sidebar end-->
        <!--main content start-->
        <section id="main-content">
            @RenderBody()
        </section>
        <!--main content end-->
        <!--footer start-->
        <footer class="site-footer">
            <div class="text-center">
                2019 &copy;
                <a href="#" class="go-top">
                    <i class="icon-angle-up"></i>
                </a>
            </div>
        </footer>
        <!--footer end-->
    </section>


    <script src="~/Areas/Admin/AdminContent/assets/js/jquery.js"></script>
    <script src="~/Areas/Admin/AdminContent/assets/js/jquery-1.8.3.min.js"></script>
    <script src="~/Areas/Admin/AdminContent/assets/js/bootstrap.min.js"></script>
    <script class="include" type="text/javascript" src="~/Areas/Admin/AdminContent/assets/js/jquery.dcjqaccordion.2.7.js"></script>
    <script src="~/Areas/Admin/AdminContent/assets/js/jquery.scrollTo.min.js"></script>
    <script src="~/Areas/Admin/AdminContent/assets/js/jquery.nicescroll.js" type="text/javascript"></script>
    <script src="~/Areas/Admin/AdminContent/assets/js/jquery.sparkline.js" type="text/javascript"></script>
    <script src="~/Areas/Admin/AdminContent/assets/jquery-easy-pie-chart/jquery.easy-pie-chart.js"></script>
    <script src="~/Areas/Admin/AdminContent/assets/js/owl.carousel.js"></script>
    <script src="~/Areas/Admin/AdminContent/assets/js/jquery.customSelect.min.js"></script>
    <script src="~/Areas/Admin/AdminContent/assets/js/respond.min.js"></script>
    <script class="include" type="text/javascript" src="~/Areas/Admin/AdminContent/assets/js/jquery.dcjqaccordion.2.7.js"></script>
    <script src="~/Areas/Admin/AdminContent/assets/js/common-scripts.js"></script>
    <script src="~/Areas/Admin/AdminContent/assets/js/sparkline-chart.js"></script>
    <script src="~/Areas/Admin/AdminContent/assets/js/easy-pie-chart.js"></script>
    <script src="~/Areas/Admin/AdminContent/assets/js/count.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.11.0/umd/popper.min.js"></script>
    <link href="https://cdnjs.cloudflare.com/ajax/libs/summernote/0.8.11/summernote-bs4.css" rel="stylesheet">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/summernote/0.8.11/summernote-bs4.js"></script>
    <script>
        $(document).ready(function () {
            $('.summernote').summernote({
                height: 150

            });

        });


    </script>
    <script>

        $(document).ready(function () {
            $("#owl-demo").owlCarousel({
                navigation: true,
                slideSpeed: 300,
                paginationSpeed: 400,
                singleItem: true,
                autoPlay: true

            });
        });

        //custom select box

        $(function () {
            $('select.styled').customSelect();
        });

    </script>
</body>
</html>
